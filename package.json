  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cp -r src/* dist/",
    "cp-2src-htm": "cp -r node_modules/htm/dist/htm.module.js src/js/vendor/",
    "cp-2dist-htm": "cp -r node_modules/htm/dist/htm.module.js dist/js/vendor/",
    "cp-2src-react": "cp -r node_modules/es-react/dev src/js/vendor/es-react",
    "cp-2dist-react": "cp -r node_modules/es-react dist/js/vendor/es-react",
    "serve-frontend-src": "npm run cp-2src-htm && npm run cp-2src-react && browser-sync start --server 'src' --files 'src'",
    "serve-frontend-dist": "npm run build && npm run cp-2dist-htm && npm run cp-2dist-react && browser-sync start --server 'dist' --files 'dist'",
    "serve-backend": "node server/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "browser-sync": "^2.26.7",
    "es-react": "^16.12.0",
    "htm": "^3.0.3"
  }
}
