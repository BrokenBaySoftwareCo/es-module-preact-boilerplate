{
  "name": "es-module-preact-boilerplate",
  "description": "For making new Preact projects with server-side rendering, es modules and NO compilation during development",
  "version": "1.0.0",
  "author": "Kynan Stewart",
  "bugs": {
    "url": "https://github.com/k7n4n5t3w4rt/es-module-preact-boilerplate/issues"
  },
  "dependencies": {
    "finalhandler": "^1.1.2",
    "history": "^4.10.1",
    "htm": "^3.0.4",
    "preact": "^10.4.1",
    "preact-render-to-string": "^5.1.6",
    "preact-router": "^3.2.1",
    "serve-static": "^1.14.1",
    "simplestyle-js": "github:k7n4n5t3w4rt/simplestyle#dist"
  },
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "faucet": "0.0.1",
    "fixpack": "^3.0.6",
    "flow-bin": "^0.123.0",
    "glob": "^7.1.6",
    "nodemon": "^2.0.2",
    "prettier": "1.19.1",
    "should": "^13.2.3",
    "snowpack": "^1.7.1"
  },
  "homepage": "https://github.com/k7n4n5t3w4rt/es-module-preact-boilerplate#readme",
  "keywords": [
    "boilerplate",
    "es",
    "modules"
  ],
  "license": "ISC",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/k7n4n5t3w4rt/es-module-preact-boilerplate.git"
  },
  "scripts": {
    "browsersync": "browser-sync start --server --single --files \"./\"",
    "build": "cp -r *.html *.txt *.png *.xml css img js site.webmanifest dist/",
    "fixpack": "fixpack",
    "flow": "flow check",
    "generate": "node --no-warnings server/generate.js",
    "clear-cache": "node --no-warnings server/clear_cache.js",
    "snowpack": "snowpack install --clean",
    "start": "node server/index.js",
    "test": "node --no-warnings server/testyrunner.js | faucet",
    "unlink": "node --no-warnings server/unlink.js"
  },
  "snowpack": {
    "webDependencies": [
      "history",
      "htm",
      "preact",
      "preact/hooks",
      "preact-render-to-string",
      "preact-router",
      "should/as-function.js",
      "simplestyle-js"
    ],
    "dedupe": []
  },
  "type": "module"
}
